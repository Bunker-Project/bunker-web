  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage --coverageReporters=cobertura",
    "eject": "react-scripts eject",
    "test-dev" : "react-scripts test --env=jest-environment-jsdom-sixteen"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:jsx-a11y/strict"
    ],
    "plugins": [
      "jsx-a11",
      "testing-library",
      "jest-dom"
    ],
    "rules": {
      "jsx-a11/rule-name": 2
    }
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/config/",
      "<rootDir>/src/serviceWorker.js"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^3.2.1",
    "eslint": "^6.8.0",
    "eslint-plugin-jest-dom": "^2.1.0",
